{
  "name": "pokemon-stats-checker",
  "displayName": "pokemon-stats-checker",
  "description": "",
  "version": "1.0.1",
  "icon": "img/logo@3x.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Najah7/Pokemon-Stats-Checker"
  },
  "publisher": "pokemon-stats-checker",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:base-stats-checker-sidebar-view"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "base-stats-checker-sidebar-view",
          "title": "base-stats-checker",
          "icon": "media/pokemon.svg"
        }
      ]
    },
    "views": {
      "base-stats-checker-sidebar-view": [
        {
          "type": "webview",
          "id": "base-stats-checker-sidebar",
          "name": "Quize",
          "icon": "media/heart.svg",
          "contextualTitle": "Quize"
        },
        {
          "type": "webview",
          "id": "base-stats-checker-image",
          "name": "一度プレイされた方",
          "icon": "media/heart.svg",
          "contextualTitle": "一度プレイされた方"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/diff": "^5.0.8",
    "@types/jest": "^29.5.10",
    "@types/mocha": "^10.0.3",
    "@types/node": "^18.18.13",
    "@types/vscode": "^1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/request": "^2.48.12",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "request": "^2.88.2"
  }
}
